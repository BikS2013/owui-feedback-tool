{
  "name": "owui-feedback-backend",
  "version": "1.0.0",
  "description": "Backend API for OWUI Feedback application",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "dev:tsx": "tsx watch src/index.ts",
    "build": "tsc && npm run copy-prompts",
    "copy-prompts": "mkdir -p dist/prompts && cp -r src/prompts/* dist/prompts/ 2>/dev/null || true",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.21",
    "@langchain/community": "^0.3.45",
    "@langchain/google-genai": "^0.2.10",
    "@langchain/openai": "^0.5.12",
    "@types/pg": "^8.15.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "js-yaml": "^4.1.0",
    "langchain": "^0.3.27",
    "pg": "^8.16.2",
    "puppeteer": "^24.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^9.18.0",
    "globals": "^16.2.0",
    "nodemon": "^3.1.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
